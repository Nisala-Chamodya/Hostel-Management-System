<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="467.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.blacky.hostel.controller.SearchStudentFormController">
   <children>
      <TableView fx:id="tblStudent" layoutX="34.0" layoutY="144.0" opacity="0.61" prefHeight="241.0" prefWidth="630.0">
        <columns>
          <TableColumn fx:id="colStudentId" prefWidth="75.0" text="studentId" />
          <TableColumn fx:id="colName" prefWidth="75.0" text="name" />
            <TableColumn fx:id="colAddress" prefWidth="75.0" text="address" />
            <TableColumn fx:id="colContatNo" prefWidth="112.0" text="contactNo" />
            <TableColumn fx:id="colDOB" minWidth="0.0" prefWidth="85.0" text="dob" />
            <TableColumn fx:id="colGender" prefWidth="176.0" text="gender" />
        </columns>
      </TableView>
      <Label layoutX="257.0" layoutY="32.0" opacity="0.62" text="View Students" textFill="#457ceb">
         <font>
            <Font name="Bell MT" size="35.0" />
         </font>
      </Label>
      <ImageView fitHeight="38.0" fitWidth="50.0" layoutX="209.0" layoutY="37.0" opacity="0.62" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/student.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fitHeight="38.0" fitWidth="50.0" layoutX="24.0" layoutY="31.0" onMouseClicked="#backBtnOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/backbtn.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
